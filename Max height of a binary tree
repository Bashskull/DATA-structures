/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode() : val(0), left(nullptr), right(nullptr) {}
 *     TreeNode(int x) : val(x), left(nullptr), right(nullptr) {}
 *     TreeNode(int x, TreeNode *left, TreeNode *right) : val(x), left(left), right(right) {}
 * };
 */
class Solution {
public:
    int maxDepth(TreeNode* root) {
        if(!root) return 0;
        int lh=maxDepth(root->left);
        int rh=maxDepth(root->right);
        return 1+max(lh,rh);
    }
};



my answer-(bad tc/sc)
/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode() : val(0), left(nullptr), right(nullptr) {}
 *     TreeNode(int x) : val(x), left(nullptr), right(nullptr) {}
 *     TreeNode(int x, TreeNode *left, TreeNode *right) : val(x), left(left), right(right) {}
 * };
 */
class Solution {
public:
    void recurse(TreeNode* node,vector<int>& mini,vector<vector<int>>& solver){
        if(!node) return;
        mini.push_back(node->val);
        if(!node->left && !node->right){
            solver.push_back(mini);
        }
        recurse(node->left,mini,solver);
        recurse(node->right,mini,solver);
        mini.pop_back();
    }
    
    int maxDepth(TreeNode* root) {
        if(!root) return 0;
        if(root->left==NULL && root->right==NULL) return 1;
        vector<vector<int>> solver;
        vector<int> mini;
        recurse(root,mini,solver);
        vector<int> counter;
        for(auto it:solver){
            counter.push_back(it.size());
        }
        //the one i did-
        // for(auto it:solver){
        //     int count=0;
        //     for(auto it2:it){
        //         count++;
        //     }
        //     counter.push_back(count);
        // }
        return *max_element(counter.begin(),counter.end());
    }
};
