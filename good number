class Solution {
public:
const long long MOD=1000000007;
    long long myPow(long long x, long long n) {
        if(n==0) return 1;
        if (n==1) return x % MOD;
        long long ans=1;
        long long know=myPow(x,n/2)%MOD;
        if(n%2!=0) ans=(ans*x)%MOD;
        return (ans*(know*know%MOD))%MOD;

}
    int countGoodNumbers(long long n) {
        long long ans=myPow(20,n/2) % MOD;
        if (n%2!=0) ans=ans*5%MOD;
        return (int)ans%MOD;
    }
};
