class Solution {
public:
    int repeatedStringMatch(string a, string b) {
        string au=a;
        int count=1;
        int maxreps=b.size()/a.size()+3;
        while(count<maxreps && au.find(b)==-1){
            au+=a;
            count++;
        }
        if (au.find(b) == -1) return -1;
        return count;
    }
};
